{
  "name": "@softwareventures/timestamp",
  "version": "0.0.0-development",
  "description": "Data type representing an instant in time",
  "keywords": [
    "TypeScript",
    "date",
    "date-time",
    "time",
    "timestamp"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/timestamp",
  "bugs": "https://github.com/softwareventures/timestamp/issues",
  "repository": "github:softwareventures/timestamp",
  "license": "ISC",
  "scripts": {
    "fix": "tsc --noEmit && eslint . --fix && prettier --write .",
    "lint": "tsc --noEmit && eslint . && prettier --check .",
    "prepare": "tsc",
    "semantic-release": "semantic-release",
    "test": "ava"
  },
  "sideEffects": false,
  "engines": {
    "node": "^14 || ^16 || >=18"
  },
  "dependencies": {
    "@softwareventures/date": "^1.4.2 || ^2.0.0 || ^3.0.0",
    "@softwareventures/format-timestamp": "^1.0.0",
    "@softwareventures/iterable": "^1.1.2 || ^2.0.0 || ^3.0.0",
    "@softwareventures/nullable": "^1.3.1 || ^2.0.0 || ^3.0.0",
    "@softwareventures/ordered": "^0.2.2 || ^1.0.0 || ^2.0.0",
    "@softwareventures/time": "^1.0.0 || ^2.0.0",
    "tslib": "2.6.2",
    "unknown": "^0.2.5"
  },
  "devDependencies": {
    "@softwareventures/eslint-config": "7.1.4",
    "@softwareventures/prettier-config": "4.0.0",
    "@softwareventures/semantic-release-config": "3.1.0",
    "@softwareventures/tsconfig": "7.1.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "ava": "5.3.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.55.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsdoc": "40.3.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-sonarjs": "0.23.0",
    "prettier": "2.8.8",
    "semantic-release": "19.0.5",
    "ts-node": "10.9.2",
    "typescript": "5.2.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@softwareventures"
  },
  "prettier": "@softwareventures/prettier-config",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/files"
    ]
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  },
  "publishConfig": {
    "access": "public"
  }
}
