{
  "name": "@softwareventures/timestamp",
  "version": "0.0.0-development",
  "description": "Data type representing an instant in time",
  "keywords": [
    "TypeScript",
    "date",
    "date-time",
    "time",
    "timestamp"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/timestamp",
  "bugs": "https://github.com/softwareventures/timestamp/issues",
  "repository": "github:softwareventures/timestamp",
  "license": "ISC",
  "scripts": {
    "fix": "tsc --noEmit && eslint . --fix && prettier --write .",
    "lint": "tsc --noEmit && eslint . && prettier --check .",
    "prepare": "tsc",
    "semantic-release": "semantic-release",
    "test": "ava"
  },
  "sideEffects": false,
  "engines": {
    "node": "^12 || ^14 || >=16"
  },
  "dependencies": {
    "@softwareventures/date": "^1.4.2 || ^2.0.0",
    "@softwareventures/format-timestamp": "^1.0.0-alpha.1",
    "@softwareventures/iterable": "^1.1.2 || ^2.0.0",
    "@softwareventures/nullable": "^1.3.1 || ^2.0.0",
    "@softwareventures/ordered": "^0.2.2 || ^1.0.0",
    "@softwareventures/time": "^0.8.4",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@softwareventures/eslint-config": "4.3.1",
    "@softwareventures/prettier-config": "1.0.3",
    "@softwareventures/semantic-release-config": "2.0.0",
    "@softwareventures/tsconfig": "5.1.0",
    "ava": "3.15.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "prettier": "2.4.1",
    "semantic-release": "17.4.7",
    "ts-node": "10.3.0",
    "typescript": "4.4.4"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@softwareventures"
  },
  "prettier": "@softwareventures/prettier-config",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/files"
    ]
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  },
  "publishConfig": {
    "access": "public"
  }
}
